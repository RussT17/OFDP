.main-logo {
  background: url("<%= asset_path('logo-ofdp.png') %>") no-repeat;
  width:423px;
  height:193px;
}

.main-logo-small {
  background: url("<%= asset_path('logo-ofdp-smallest.png') %>") no-repeat;
  width:200px;
  height:91px;
  margin-left:10px;
  margin-top:0px;
}

.home-container * {
  margin-left:auto;
  margin-right:auto;
  text-align:center;
}

.home-container {
  padding-top:125px;
  background-color:white;
}

body {
  font-family:sans-serif;
  background-color:#333333;
  margin:0;
}

.container {
  width:960px;
  margin-left:auto;
  margin-right:auto;
  background-color:#eeeeee;
}

.header {
  background-color:#eeeeee;
}

.side-bar {
  width:220px;
  height:inherit;
  background-color:#dddddd;
  float:left;
}

.filler {
  background-color:#dddddd;
}

.footer {
  background-color:#eeeeee;
  height:45px;
  width:960px;
  bottom:0px;
  margin-left:auto;
  margin-right:auto;
  position:relative;
  display:block;
}

.page-content {
  padding:45px;
  padding-top:15px;
  background-color:white;
  border:groove;
  border-right:none;
  margin-left:220px;
  min-height:600px;
}

.data-selector {
}

.footer {
  clear:both;
}