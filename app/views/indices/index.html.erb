<h1 class='page-title'>Table of Contents</h1>
	<%= paginate @contents %><br>
	<table>
		<tr>
			<th>
				Dataset Code
			</th>
			<th>
				Name
			</th>
			<th>
				Description
			</th>
			<th>
				Data URL
			</th>
		</tr>
		<% for c in @contents %>
			<td>index.<%= c.name.split(' ').map{|w| w[0]}.join + '.' + c.id.to_s %></td>
			<td><%= c.name %></td>
			<td>Source: DryShips Inc.</td>
			<td><%= link_to index_url(c), index_url(c) %></td>
		</tr>
		<% end %>
	</table>